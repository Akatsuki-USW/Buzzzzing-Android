<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SnsLoginButton">
        <attr name="loginButtonBackground" format="reference|integer" />
        <attr name="loginButtonProgressBarTint" format="reference|color" />
        <attr name="loginButtonSnsLogo" format="reference|integer" />
        <attr name="loginButtonText" format="reference|string" />
        <attr name="loginButtonTextColor" format="reference|integer" />

    </declare-styleable>

    <declare-styleable name="FigmaTextView">
        <attr name="figmaLineHeight" format="float" />
    </declare-styleable>

    <declare-styleable name="TextInputLayout">
        <attr name="label" format="reference|string" />
        <attr name="hideLabel" format="boolean" />
        <attr name="helperTextContent" format="reference|string" />
        <attr name="hideHelperTextContent" format="boolean" />
    </declare-styleable>
</resources>